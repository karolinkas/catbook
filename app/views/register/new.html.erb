<h1>Register#new</h1>

<%= form_for @cat, url: register_index_path do |f| %>

  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.text_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
<p>Find me in app/views/register/new.html.erb</p>
